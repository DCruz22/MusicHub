@model IEnumerable<MusicHub.Models.User>

@{
    Layout = "~/Views/Shared/_Logged_Layout.cshtml";
}

<h2>My followers</h2>
<div class="row">
    <div class="container">
        @if (Model.Count() > 0)
        {
            @Html.Partial("_UserFollow", Model)
        }
        else
        {
            <span>No one is following you :(</span>
        }
    </div>
</div>
